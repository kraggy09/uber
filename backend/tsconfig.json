{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "rootDir": "./src",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true ,
    "allowSyntheticDefaultImports": true,
  },
  "include": ["src/**/*","src/types/**/*"],
  "exclude": ["node_modules", "dist"]
}
